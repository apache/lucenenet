{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Lucene.Net.ICU/Lucene.Net.ICU.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ],
          "src": "../../src/dotnet"
        }
      ],
      "dest": "obj/docfx/api/icu",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "overview.md"
        ],
        "src": "../../src/dotnet/Lucene.Net.ICU"
      },
      {
        "files": [
          "**.yml",
          "**.md"
        ],
        "src": "obj/docfx/api/icu"
      },   
      {
        "files": [
          "toc.yml",
          "icu/toc.yml"
        ],
        "src": "toc"
      }
    ],    
    "overwrite": [
      {
        "files": [
          "**/package.md",
          "**/overview.md"
        ],
        "src": "../../src/Lucene.Net.Analysis.ICU",
        "exclude": [
          "package.md"
        ]
      },     
      {
        "files": [
          "apiSpec/icu/**/*.md"
        ]
      }
    ],
    "xref": [
      "_site/api/core/xrefmap.yml"
    ],
    "dest": "_site/api/icu",
    "globalMetadataFiles": [
      "docfx.global.json",
      "docfx.global.subsite.json"
    ],
    "template": [
      "Templates/DefaultTemplateNoAssets",
      "Templates/LuceneTemplate",
      "Templates/LuceneApiDocs"
    ],
    "postProcessors": [],
    "markdownEngineName": "dfm",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}