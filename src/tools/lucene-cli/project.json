{
  "version": "4.8.0",
  "entryPoint": "Program",
  "buildOptions": {
    "emitEntryPoint": true,
    "compile": {
      "includeFiles": [
        "../../CommonAssemblyInfo.cs"
      ]
    },
    "embed": {
      "include": [
        "../../Lucene.Net.Demo/*.cs",
        "../../Lucene.Net.Demo/Facet/*.cs"
      ]
    }
  },

  "dependencies": {
    "Lucene.Net": "4.8.0",
    "Lucene.Net.Analysis.Common": "4.8.0",
    "Lucene.Net.Analysis.Stempel": "4.8.0",
    "Lucene.Net.Demo": "4.8.0",
    "Lucene.Net.Expressions": "4.8.0",
    "Lucene.Net.Facet": "4.8.0",
    "Lucene.Net.Misc": "4.8.0",
    "Lucene.Net.QueryParser": "4.8.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.1"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50"
    }
  }
}
